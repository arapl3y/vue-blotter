{
  "name": "vue-blotter",
  "version": "0.1.0",
  "author": {
    "name": "Alex Rapley",
    "email": "rapley3@gmail.com"
  },
  "repository": "https://github.com/arapl3y/vue-blotter",
  "bugs": "https://github.com/arapl3y/vue-blotter/issues",
  "license": "MIT",
  "description": "A Blotter.js Vue component",
  "keywords": [
    "vue",
    "blotter",
    "blotter.js",
    "vue-blotter",
    "glsl"
  ],
  "main": "dist/vue-blotter.ssr.js",
  "module": "dist/vue-blotter.esm.js",
  "unpkg": "dist/vue-blotter.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "rollup": "^1.12.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^5.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
